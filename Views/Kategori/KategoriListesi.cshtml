@model IEnumerable<BlogProje.Models.Kategori>

<div class="well">
    <h4>Yazı Kategorileri</h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li>
                        <a href="/Yazi/Liste/@item.Id">@item.KategoriAdi</a>
                    </li>
                </ul>
            </div>
        }
    </div>
</div>